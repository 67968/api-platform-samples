<AssignMessage async="false" continueOnError="false" enabled="true" name="SetResponseHeaders">
    <DisplayName>Set Response Headers</DisplayName>
    <Properties/>
    <Set>
	    <Headers>
	      <Header name="cache_hit">{responsecache.ResponseCache.cachehit}</Header>
	      <Header name="cache_name">{responsecache.ResponseCache.cachename}</Header>
	      <Header name="cache_key">{responsecache.ResponseCache.cachekey}</Header>
	      <Header name="cache_invalidentry">{responsecache.ResponseCache.invalidentry}</Header>
	    </Headers>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="responset"/>
</AssignMessage>
