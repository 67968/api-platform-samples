<ProxyEndpoint name="proxy">
  <PreFlow>
    <Response>
      <Step>
        <Name>javacallout</Name>		
      </Step>
    </Response>
  </PreFlow>
  <HTTPProxyConnection>
    <BasePath>/java-callout</BasePath>
    <VirtualHost>default</VirtualHost>
  </HTTPProxyConnection>
  <RouteRule name="default">
    <TargetEndpoint>default</TargetEndpoint>
  </RouteRule>
</ProxyEndpoint>
